{
  "compilerOptions": {
    "allowJs": true,
    "checkJs": false,
    "downlevelIteration": true,
    "esModuleInterop": true,
    "incremental": true,
    "isolatedModules": true,
    "lib": ["es6"],
    "module": "es2015",
    "moduleResolution": "node",
    "noEmit": false,
    "plugins": [],
    "skipDefaultLibCheck": true,
    "skipLibCheck": false,
    "sourceMap": true,
    "target": "ES2015",
    "typeRoots": ["node_modules/@types"]
  },
  "exclude": ["**/node_modules", "**/.*/"],
  "include": ["code", "lib"]
}
